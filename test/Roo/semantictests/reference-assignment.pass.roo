array[5] integer buf5;

procedure main ()
    buf5 result;
    boolean result2;
{
    call proc(result[2], result2, result[1], true);
}

procedure proc (integer n, boolean x, integer val m, boolean val y)
{
    writeln "Hello from Procedure";
    write "Integer ref arg is: ";
    writeln n;
    write "Boolean ref arg is: ";
    writeln x;
    write "Integer val arg is: ";
    writeln m;
    write "Boolean val arg is: ";
    writeln y;
}